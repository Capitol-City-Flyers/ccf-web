    "tailwindcss": "^3.2.7",
    "tokenizr": "^1.6.9",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5",
    "webpack-cli": "^5.0.1",
    "workbox-webpack-plugin": "^6.5.4"
  },
  "jest": {
    "reporters": [
      "default",
      "jest-junit",
      [
        "jest-html-reporters",
        {
          "filename": "index.html",
          "publicPath": "./build/reports/tests/webpackTest"
        }
      ]
    ],
    "testRegex": "/src/test/webpack/.+\\.test\\.[jt]sx?$",
    "moduleDirectories": [
      "node_modules",
      "<rootDir>/src/main/webpack"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "ts",
      "tsx"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "jest-junit": {
    "outputDirectory": "build/test-results/webpackTest",
    "usePathForSuiteName": "true"
  }
}
